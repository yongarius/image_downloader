{
  "name": "My Image Downloader",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "My Image Downloader",
  "icons": {
    "16": "icons/nasapod16x16.png",
    "32": "icons/nasapod32x32.png",
    "48": "icons/nasapod48x48.png",
    "128": "icons/nasapod128x128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/jquery.min.js", "content-scripts/content.js"]
    }
  ]
}
